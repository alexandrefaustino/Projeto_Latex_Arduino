%imprime os elementos textuais
\newcommand{\imprimirTexto}{
    \newpage    % a introdução será iniciada em uma nova página
    \begin{multicols}{2} %Divide a folha em duas colunas
        %inclusão da introdução
        \input{2-textuais/1-introducao}
        
        %inclusão do referencial teórico
        \input{2-textuais/2-referencialTeorico}
        
        %inclusão dos Métodos da pesquisa
        \input{2-textuais/3-metodoPesquisa}
        
        %inclusão dos Resultados da pesquisa
        \input{2-textuais/4-resultadosPesquisa}
        
         %inclusão da conclusão
        \input{2-textuais/5-conclusao}
    \end{multicols}
}

